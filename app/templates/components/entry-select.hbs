<div class='row selectable-entry'>
  <div class='col-xs-12'>
    <div class='row'>
      <div class='col-xs-12'>
        <div class='title'>{{entry.title}}</div>
      </div>
      <div class='col-xs-6'>
        <div class='author'>{{entry.userName}}</div>
      </div>
      <div class='col-xs-6'>
        <div class='modified'>{{moment-time entry.modified}}</div>
      </div>
    </div>
  </div>
</div>

